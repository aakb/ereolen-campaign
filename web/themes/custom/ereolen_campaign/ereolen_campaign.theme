<?php

/**
 * Implements hook_preprocess_region().
 *
 * Add header text from variable.
 *
 * @param $variables
 */
function ereolen_campaign_preprocess_region__header(&$variables) {
  $config = \Drupal::getContainer()->get('itk_hero.config');
  $variables['header_text'] = $config->get('itk_header_text') ? $config->get('itk_header_text') : FALSE;
}